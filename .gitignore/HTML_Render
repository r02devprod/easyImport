<!DOCTYPE html>
<html>

<head>
</head>

<body>
    <script>
        function Create() {};

        Create.prototype.node = (nodeName, content) => {
            let n = document.createElement(nodeName);

            if (content == undefined) {
                content = "";
            };

            let t = document.createTextNode(content);
            n.appendChild(t);
            return n;
        };

        Create.prototype.multiNodes = (starts, ends, func) => {
            for (starts; starts < ends; starts++) {
                func(starts);
            }
        };

        let create = new Create();
    </script>


    <script>
        (HTMLRender = () => {
            let div = create.node("h1", "What the hell bro?");
            document.body.insertAdjacentHTML("beforeend");




        })();
    </script>
</body>

</html>
